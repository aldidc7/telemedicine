{
  "openapi": "3.0.0",
  "info": {
    "title": "Telemedicine API",
    "description": "REST API untuk aplikasi telemedicine RSUD dr. R. Soedarsono",
    "version": "2.0.0",
    "contact": {
      "name": "API Support",
      "email": "support@telemedicine.local"
    },
    "license": {
      "name": "MIT"
    }
  },
  "servers": [
    {
      "url": "http://localhost:8000/api",
      "description": "Development server"
    },
    {
      "url": "https://api.telemedicine.local/api",
      "description": "Production server"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      },
      "apiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "X-API-Key"
      }
    },
    "schemas": {
      "ApiResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "example": true
          },
          "pesan": {
            "type": "string",
            "example": "Success"
          },
          "data": {
            "type": "object"
          },
          "error_code": {
            "type": "string",
            "example": "GENERAL_ERROR"
          },
          "status_code": {
            "type": "integer",
            "example": 200
          }
        }
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "example": 1
          },
          "name": {
            "type": "string",
            "example": "Ahmad Zaki"
          },
          "email": {
            "type": "string",
            "example": "ahmad@example.com"
          },
          "role": {
            "type": "string",
            "enum": ["pasien", "dokter", "admin"],
            "example": "pasien"
          },
          "is_active": {
            "type": "boolean",
            "example": true
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "LoginRequest": {
        "type": "object",
        "required": ["email", "password"],
        "properties": {
          "email": {
            "type": "string",
            "format": "email",
            "example": "ahmad@example.com"
          },
          "password": {
            "type": "string",
            "format": "password",
            "minLength": 8,
            "example": "password123"
          }
        }
      },
      "LoginResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "example": true
          },
          "pesan": {
            "type": "string",
            "example": "Login berhasil"
          },
          "data": {
            "type": "object",
            "properties": {
              "user": {
                "$ref": "#/components/schemas/User"
              },
              "token": {
                "type": "string",
                "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
              },
              "expires_in": {
                "type": "integer",
                "example": 3600
              }
            }
          }
        }
      },
      "Pasien": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "user_id": {
            "type": "integer"
          },
          "medical_record_number": {
            "type": "string",
            "example": "RM-2025-00001"
          },
          "encrypted_nik": {
            "type": "string"
          },
          "date_of_birth": {
            "type": "string",
            "format": "date"
          },
          "gender": {
            "type": "string",
            "enum": ["male", "female"]
          },
          "address": {
            "type": "string"
          },
          "phone_number": {
            "type": "string"
          },
          "blood_type": {
            "type": "string",
            "enum": ["A", "B", "AB", "O"]
          }
        }
      },
      "Dokter": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "user_id": {
            "type": "integer"
          },
          "license_number": {
            "type": "string"
          },
          "specialization": {
            "type": "string",
            "example": "Dokter Umum"
          },
          "is_available": {
            "type": "boolean"
          },
          "is_verified": {
            "type": "boolean"
          },
          "years_of_experience": {
            "type": "integer"
          },
          "bio": {
            "type": "string"
          }
        }
      },
      "ValidationError": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "example": false
          },
          "pesan": {
            "type": "string",
            "example": "Validasi gagal"
          },
          "error_code": {
            "type": "string",
            "example": "VALIDATION_FAILED"
          },
          "errors": {
            "type": "object",
            "example": {
              "email": ["Email harus diisi"],
              "password": ["Password minimal 8 karakter"]
            }
          }
        }
      }
    }
  },
  "paths": {
    "/v1/health": {
      "get": {
        "summary": "Health check",
        "description": "Cek apakah API berjalan",
        "tags": ["System"],
        "responses": {
          "200": {
            "description": "API berjalan normal"
          }
        }
      }
    },
    "/v1/auth/login": {
      "post": {
        "summary": "Login user",
        "description": "Authenticate dan dapatkan access token",
        "tags": ["Auth"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login berhasil",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginResponse"
                }
              }
            }
          },
          "401": {
            "description": "Email atau password tidak valid"
          },
          "422": {
            "description": "Validasi gagal",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/auth/register": {
      "post": {
        "summary": "Register user baru",
        "description": "Registrasi akun pasien baru",
        "tags": ["Auth"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["name", "email", "password"],
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "Ahmad Zaki"
                  },
                  "email": {
                    "type": "string",
                    "format": "email"
                  },
                  "password": {
                    "type": "string",
                    "minLength": 8
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Registrasi berhasil"
          },
          "422": {
            "description": "Validasi gagal atau email sudah terdaftar"
          }
        }
      }
    },
    "/v1/auth/me": {
      "get": {
        "summary": "Get current user",
        "description": "Dapatkan profil user yang sedang login",
        "tags": ["Auth"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "User profile",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ApiResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/v1/auth/logout": {
      "post": {
        "summary": "Logout user",
        "description": "Logout dan revoke token",
        "tags": ["Auth"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Logout berhasil"
          }
        }
      }
    },
    "/v1/auth/refresh": {
      "post": {
        "summary": "Refresh token",
        "description": "Refresh access token",
        "tags": ["Auth"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Token baru"
          }
        }
      }
    },
    "/v1/pasien": {
      "get": {
        "summary": "List patients",
        "description": "Dapatkan daftar semua pasien (Admin only)",
        "tags": ["Pasien"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "description": "Halaman (default: 1)",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "per_page",
            "in": "query",
            "description": "Jumlah data per halaman (default: 15)",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "search",
            "in": "query",
            "description": "Cari berdasarkan nama atau email",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Daftar pasien"
          },
          "403": {
            "description": "Forbidden - hanya admin"
          }
        }
      },
      "post": {
        "summary": "Create patient",
        "description": "Buat profil pasien baru",
        "tags": ["Pasien"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["date_of_birth", "gender", "blood_type"],
                "properties": {
                  "date_of_birth": {
                    "type": "string",
                    "format": "date"
                  },
                  "gender": {
                    "type": "string",
                    "enum": ["male", "female"]
                  },
                  "blood_type": {
                    "type": "string",
                    "enum": ["A", "B", "AB", "O"]
                  },
                  "address": {
                    "type": "string"
                  },
                  "phone_number": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Pasien berhasil dibuat"
          }
        }
      }
    },
    "/v1/pasien/{id}": {
      "get": {
        "summary": "Get patient detail",
        "description": "Dapatkan detail pasien berdasarkan ID",
        "tags": ["Pasien"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "Patient ID",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Detail pasien"
          },
          "404": {
            "description": "Pasien tidak ditemukan"
          }
        }
      },
      "put": {
        "summary": "Update patient",
        "description": "Update informasi pasien",
        "tags": ["Pasien"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "date_of_birth": {
                    "type": "string",
                    "format": "date"
                  },
                  "gender": {
                    "type": "string"
                  },
                  "blood_type": {
                    "type": "string"
                  },
                  "address": {
                    "type": "string"
                  },
                  "phone_number": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Pasien berhasil diupdate"
          }
        }
      },
      "delete": {
        "summary": "Delete patient",
        "description": "Hapus pasien",
        "tags": ["Pasien"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Pasien berhasil dihapus"
          }
        }
      }
    },
    "/v1/pasien/{id}/medical-records": {
      "get": {
        "summary": "Get patient medical records",
        "description": "Dapatkan riwayat medis pasien",
        "tags": ["Pasien"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Riwayat medis"
          }
        }
      }
    },
    "/v1/dokter": {
      "get": {
        "summary": "List doctors",
        "description": "Dapatkan daftar dokter (public)",
        "tags": ["Dokter"],
        "parameters": [
          {
            "name": "specialization",
            "in": "query",
            "description": "Filter berdasarkan spesialisasi",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "is_available",
            "in": "query",
            "description": "Filter berdasarkan ketersediaan",
            "schema": {
              "type": "boolean"
            }
          },
          {
            "name": "page",
            "in": "query",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Daftar dokter"
          }
        }
      },
      "post": {
        "summary": "Create doctor profile",
        "description": "Daftar sebagai dokter",
        "tags": ["Dokter"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["license_number", "specialization"],
                "properties": {
                  "license_number": {
                    "type": "string"
                  },
                  "specialization": {
                    "type": "string"
                  },
                  "years_of_experience": {
                    "type": "integer"
                  },
                  "bio": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Profil dokter berhasil dibuat"
          }
        }
      }
    },
    "/v1/dokter/{id}": {
      "get": {
        "summary": "Get doctor detail",
        "description": "Dapatkan detail dokter",
        "tags": ["Dokter"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Detail dokter"
          }
        }
      },
      "put": {
        "summary": "Update doctor",
        "description": "Update profil dokter",
        "tags": ["Dokter"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "specialization": {
                    "type": "string"
                  },
                  "years_of_experience": {
                    "type": "integer"
                  },
                  "bio": {
                    "type": "string"
                  },
                  "is_available": {
                    "type": "boolean"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Dokter berhasil diupdate"
          }
        }
      }
    },
    "/v1/dokter/{id}/schedule": {
      "get": {
        "summary": "Get doctor schedule",
        "description": "Dapatkan jadwal dokter",
        "tags": ["Dokter"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "date",
            "in": "query",
            "schema": {
              "type": "string",
              "format": "date"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Jadwal dokter"
          }
        }
      }
    },
    "/v1/dokter/{id}/earnings": {
      "get": {
        "summary": "Get doctor earnings",
        "description": "Dapatkan penghasilan dokter (Owner only)",
        "tags": ["Dokter"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "month",
            "in": "query",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "year",
            "in": "query",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Data penghasilan"
          }
        }
      }
    },
    "/v1/dokter/{id}/ratings": {
      "get": {
        "summary": "Get doctor ratings",
        "description": "Dapatkan rating dan review dokter",
        "tags": ["Dokter"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "page",
            "in": "query",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Daftar rating dan review"
          }
        }
      }
    },
    "/v1/konsultasi": {
      "get": {
        "summary": "List consultations",
        "description": "Dapatkan daftar konsultasi user",
        "tags": ["Konsultasi"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "status",
            "in": "query",
            "description": "Filter berdasarkan status",
            "schema": {
              "type": "string",
              "enum": ["pending", "ongoing", "completed", "cancelled"]
            }
          },
          {
            "name": "page",
            "in": "query",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Daftar konsultasi"
          }
        }
      },
      "post": {
        "summary": "Create consultation",
        "description": "Buat konsultasi baru (Pasien only)",
        "tags": ["Konsultasi"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["doctor_id", "complaint_type", "description"],
                "properties": {
                  "doctor_id": {
                    "type": "integer"
                  },
                  "complaint_type": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  },
                  "appointment_date": {
                    "type": "string",
                    "format": "date"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Konsultasi berhasil dibuat"
          }
        }
      }
    },
    "/v1/konsultasi/{id}": {
      "get": {
        "summary": "Get consultation detail",
        "description": "Dapatkan detail konsultasi",
        "tags": ["Konsultasi"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Detail konsultasi"
          }
        }
      },
      "put": {
        "summary": "Update consultation",
        "description": "Update status konsultasi",
        "tags": ["Konsultasi"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "string",
                    "enum": ["pending", "ongoing", "completed", "cancelled"]
                  },
                  "diagnosis": {
                    "type": "string"
                  },
                  "treatment": {
                    "type": "string"
                  },
                  "notes": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Konsultasi berhasil diupdate"
          }
        }
      }
    },
    "/v1/konsultasi/{id}/messages": {
      "get": {
        "summary": "Get consultation messages",
        "description": "Dapatkan chat/pesan konsultasi",
        "tags": ["Konsultasi"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Daftar pesan"
          }
        }
      },
      "post": {
        "summary": "Send consultation message",
        "description": "Kirim pesan/chat dalam konsultasi",
        "tags": ["Konsultasi"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["message"],
                "properties": {
                  "message": {
                    "type": "string"
                  },
                  "attachment_url": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
